<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="catp-comp-costum-style" name="Company Costum Style" inherit_id="website.assets_frontend">
        <xpath expr="." position="inside">
            <link href="/erpi_theme_company/static/src/less/costum.less" rel="stylesheet" type="text/less"/>
        </xpath>
    </template>
    <template id="catp-comp-cdn-style" name="Company CDN  Style" inherit_id="website.assets_frontend">
        <xpath expr="//link[last()]" position="after">
            <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700|Source+Sans+Pro:400,700,900" rel="stylesheet"/>
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.0/animate.min.css" />
        </xpath>
    </template>
    <template id="catp-comp-costum-js" name="Company Costum js" inherit_id="website.assets_frontend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/erpi_theme_company/static/src/js/costum.js"></script>
        </xpath>
    </template>
    <template id="catp-comp-cdn-js" name="Company CDN js" inherit_id="website.assets_frontend">
        <xpath expr="//script[last()]" position="after">
            <script src="https://cdnjs.cloudflare.com/ajax/libs/waypoints/4.0.1/jquery.waypoints.min.js"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/nod/2.0.12/nod.min.js"></script>
        </xpath>
    </template>
    <!-- <template id="catp-comp-costum-head" name="Company Costum head" inherit_id="web.layout">
        <xpath expr="//head" position="after">
        </xpath>
    </template> -->
</odoo>